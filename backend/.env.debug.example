# Debug Environment Variables
# Copy this to your .env file and uncomment the variables you want to enable

# Enable detailed request/response debugging
# DEBUG_HEADERS=true
# DEBUG_RESPONSE=true
# DEBUG_STACK=true

# MongoDB Debug Settings
# MONGODB_DEBUG=true

# Log Levels (error, warn, log, debug, verbose)
# LOG_LEVEL=debug

# Performance Monitoring
# SLOW_QUERY_THRESHOLD=500
# SLOW_REQUEST_THRESHOLD=1000

# Memory Monitoring
# MEMORY_CHECK_INTERVAL=30000
